{
  "name": "injection-js",
  "version": "2.4.0",
  "description": "Dependency Injection library for JavaScript and TypeScript",
  "main": "injection.bundle.js",
  "module": "index.js",
  "jsnext:main": "index.js",
  "types": "index.d.ts",
  "engines": {
    "node": ">=12.13"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgechev/injection-js.git"
  },
  "dependencies": {
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rollup": "^2.41.5",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.00",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "prebuild": "npm run lint && npm run test",
    "build": "rimraf dist && tsc && rollup -c rollup.config.js -i dist/index.js > dist/injection.bundle.js && ts-node -p tools/tsconfig.json tools/copy.ts",
    "test": "jest --coverage",
    "lint": "eslint -c .eslintrc.js lib/**/*.ts"
  },
  "keywords": [
    "DI",
    "dependency",
    "injection",
    "dependency injection",
    "injector"
  ],
  "author": "Minko Gechev <mgechev@gmail.com>",
  "license": "MIT"
}
